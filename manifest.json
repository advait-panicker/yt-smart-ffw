{
    "manifest_version": 3,
    "name": "Smart FastForward",
    "version": "1.0",
    "description": "Adjusts video playback speed based on audio volume",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["*://www.youtube.com/watch?v=*"],
        "js": ["main.js"]
      }
    ],
    "options_page": "options.html"
  }
  